<div class="content">
        <div class="row">
            <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                <h4 class="card-title"> Students List</h4>
                <a href="/students/new">
              <i class="now-ui-icons ui-1_simple-add"></i>
              <p>Create</p>
            </a>
                </div>  <! -- card-header --> 
                <div class="card-body">
                <div class="table-responsive">
                  <table class="table">
                    <thead class=" text-primary">
                    <th>ID</th>
                      <th></th>
                      <th>Name</th>
                      <th>Email</th>
                      <th>Education</th>
                    </thead>
                    <tbody>
                    <% @students.each do |student|%>
                        <tr>
        <td>  <%= student.id %></td>
        <td><div class="card-avatar list mx-auto">
          <img class="avatar_sm list" src="<%= student.avatar %>">
            </div>
            </td>
        <td> <%= student.first_name %> &nbsp;
          <%= student.last_name %></td>
          <td>  <%= student.email %></td>
          <td>  <%= student.education %></td>
          <td>  <%= link_to("Show", student_path(student), :class => 'action view') %></td>
          </tr>
          <% end %>
                    </tbody>
                    </table>
            </div>       
            </div>             
        </div>  <!-- row --> 
    </div>  <!-- content -->  











